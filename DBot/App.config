<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    
    <!-- allow DM -->
    <add key="enable_DM" value="true" />

    <add key="stringprefix" value="!"/>
    
    <!-- json RPC -->
    <add key="jsonrpc_url" value="http://127.0.0.1:9918"/>
    <add key="jsonrpc_user" value="someuser" />
    <add key="jsonrpc_pass" value="somepass"/>
    
    <!-- graviex -->
    <add key="graviex_api_url" value="https://graviex.net:443//api/v2/tickers/absbtc.json"/>
    
    <!-- crex -->
    <add key="crex_api_url" value="https://api.crex24.com/v2/public/tickers?instrument=ABS-BTC"/>
    
    <!-- stock.exchange -->
    <add key="stockexchange_api_url" value="https://app.stocks.exchange/api2/ticker"/>
    <add key="stockexchange_ticker" value="ABS_BTC"/>
    
    <!-- coinmarketcap btc -->
    <add key="coinmarketcap_api_url" value="https://api.coinmarketcap.com/v2/ticker/1/?convert=EUR"/>

  </appSettings>
</configuration>